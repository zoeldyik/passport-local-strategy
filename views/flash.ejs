<% if(message.length){ %>
<div class="alert alert-<%= message[0]; %>  alert-dismissible fade show" role="alert">
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
    <strong><%= message[1]; %> </strong>
</div>
<% } %>